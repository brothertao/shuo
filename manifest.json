{
   "background_page": "background.html",
   "browser_action": {
      "default_icon": "img/icon48.png",
      "default_title": "__MSG_browserActionDefaultTitle__"
   },
   "content_scripts": [ {
      "exclude_globs": [ "http://reddit.local/*" ],
      "js": [ "js/jquery-1.6.2.js"
              , "js/jquery.ztree.core-3.0.js"
              , "js/jquery.ztree.excheck-3.0.js"
              , "js/jquery.ztree.exedit-3.0.js"
              , "js/ui/jquery-ui-1.8.16.custom.js"
              , "js/jquery.jsPlumb-1.3.3-all.js"
              , "js/script.js" 
            ],
      "matches": [ "http://*/*" ]
   } ],
   "description": "shuo is a chrome plugin for comment from ----",
   "icons": {
      "128": "img/icon128.png",
      "48": "img/icon48.png"
   },
   "name": "shuo",
